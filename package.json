{
  "name": "@damnard/node-typescript-hello-world",
  "version": "0.0.1",
  "description": "Exemple d'application Node.js avec TypeScript",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "clean": "tsc --build --clean",
    "build": "tsc --build",
    "start": "node dist/index.js",
    "fix": "prettier --write src/**/*.ts && eslint --fix src/**/*.ts",
    "lint": "eslint src/**/*.ts"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "keywords": [
    "node",
    "typescript",
    "hello-world"
  ],
  "author": "Damien Bernard",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/damien-bernard/node-typescript-hello-world.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3"
  }
}
